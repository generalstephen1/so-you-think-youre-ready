
<h2 class="point-content__title">{{content.title}}</h2>

{{#if content}}
    {{#each content.content as |block|}}
        {{#if block.type }}
        {{component (concat 'primitive/' block.type '-component') block=block}}
        {{else}}
        {{component (concat 'primitive/text-component') block=block}}
        {{/if}}
     {{/each}}
{{else}}
<p>Still working on this one!</p>
{{/if}}
