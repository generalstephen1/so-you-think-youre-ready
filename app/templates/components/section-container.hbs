
<div class="section-container row">
    <div class="side-bar col-4">
        {{side-bar
            title=model.title
            points=model.points
            loadPoint=(action "loadPoint")
        }}
    </div>

    <div class="section-content col-8">
        {{point-content point=(render-content model.points currentContentID)}}
    </div>
</div>